    "format": "prettier --write src/"
  },
  "dependencies": {
    "ant-design-vue": "4.x",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^4.0.0",
    "uuid": "^10.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.0.0",
    "sass": "^1.68.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.5.4",
    "vite": "^5.4.3",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  }
